{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/OwlCore.csproj"
          ]
        }
      ],
      "dest": "../docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "src": "../docs",
        "files": [
          "api/**.yml",
          "api/toc.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "index.md"
        ],
        "src": "../docs"
      }
    ],
    "dest": "../docs/wwwroot/",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}